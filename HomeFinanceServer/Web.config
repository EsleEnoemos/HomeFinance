	</appSettings>
	<connectionStrings>
		<add name="DBConnection" connectionString="Data Source=(local);Initial Catalog=HomeFinance;Persist Security Info=True;User ID=sa;Password=holmebodar" />
	</connectionStrings>
	<system.web>
		<compilation debug="true" targetFramework="4.0" />
